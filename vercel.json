{
    "rewrites": [
        {
            "source": "/:match*",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Permissions-Policy",
                    "value": "browsing-topics=()"
                }
            ]
        }
    ]
}